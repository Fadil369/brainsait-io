<svg xmlns="http://www.w3.org/2000/svg" width="300" height="200" viewBox="0 0 300 200" fill="none">
  <defs>
    <linearGradient id="excelGrad" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#10b981;stop-opacity:1"/>
      <stop offset="100%" style="stop-color:#059669;stop-opacity:1"/>
    </linearGradient>
  </defs>
  
  <rect width="300" height="200" fill="#f8fafc"/>
  <rect x="20" y="20" width="260" height="160" rx="8" fill="white" stroke="#e5e7eb"/>
  
  <!-- Excel spreadsheet interface -->
  <rect x="40" y="40" width="220" height="120" rx="4" fill="url(#excelGrad)" opacity="0.1"/>
  
  <!-- Grid lines -->
  <g stroke="#10b981" stroke-width="1" opacity="0.3">
    <line x1="70" y1="40" x2="70" y2="160"/>
    <line x1="110" y1="40" x2="110" y2="160"/>
    <line x1="150" y1="40" x2="150" y2="160"/>
    <line x1="190" y1="40" x2="190" y2="160"/>
    <line x1="230" y1="40" x2="230" y2="160"/>
    
    <line x1="40" y1="60" x2="260" y2="60"/>
    <line x1="40" y1="80" x2="260" y2="80"/>
    <line x1="40" y1="100" x2="260" y2="100"/>
    <line x1="40" y1="120" x2="260" y2="120"/>
    <line x1="40" y1="140" x2="260" y2="140"/>
  </g>
  
  <!-- Header row -->
  <rect x="40" y="40" width="220" height="20" fill="#10b981"/>
  <text x="55" y="53" font-family="Arial, sans-serif" font-size="8" fill="white" font-weight="bold">A</text>
  <text x="90" y="53" font-family="Arial, sans-serif" font-size="8" fill="white" font-weight="bold">B</text>
  <text x="130" y="53" font-family="Arial, sans-serif" font-size="8" fill="white" font-weight="bold">C</text>
  <text x="170" y="53" font-family="Arial, sans-serif" font-size="8" fill="white" font-weight="bold">D</text>
  <text x="210" y="53" font-family="Arial, sans-serif" font-size="8" fill="white" font-weight="bold">E</text>
  
  <!-- Data cells with colors -->
  <rect x="41" y="61" width="28" height="18" fill="#f3f4f6"/>
  <rect x="71" y="61" width="38" height="18" fill="#dbeafe"/>
  <rect x="111" y="61" width="38" height="18" fill="#dcfce7"/>
  
  <rect x="41" y="81" width="28" height="18" fill="#f3f4f6"/>
  <rect x="71" y="81" width="38" height="18" fill="#fef3c7"/>
  <rect x="111" y="81" width="38" height="18" fill="#fce7f3"/>
  
  <!-- Data processing arrows -->
  <g fill="#10b981">
    <polygon points="180,90 195,95 180,100" opacity="0.7"/>
    <polygon points="200,110 215,115 200,120" opacity="0.7"/>
  </g>
  
  <!-- Merge/consolidation symbol -->
  <circle cx="230" cy="100" r="15" fill="#10b981"/>
  <text x="230" y="105" text-anchor="middle" font-family="Arial, sans-serif" font-size="12" fill="white">âš¡</text>
  
  <!-- Title -->
  <text x="150" y="175" text-anchor="middle" font-family="Arial, sans-serif" font-size="14" font-weight="bold" fill="#1f2937">
    Excel Advanced Processor
  </text>
</svg>