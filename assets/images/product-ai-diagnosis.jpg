<svg xmlns="http://www.w3.org/2000/svg" width="300" height="200" viewBox="0 0 300 200" fill="none">
  <defs>
    <linearGradient id="aiGrad" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#2563eb;stop-opacity:1"/>
      <stop offset="100%" style="stop-color:#7c3aed;stop-opacity:1"/>
    </linearGradient>
  </defs>
  
  <rect width="300" height="200" fill="#f8fafc"/>
  <rect x="20" y="20" width="260" height="160" rx="8" fill="white" stroke="#e5e7eb"/>
  
  <!-- Central AI Brain -->
  <circle cx="150" cy="100" r="35" fill="url(#aiGrad)" opacity="0.1"/>
  <circle cx="150" cy="100" r="25" fill="url(#aiGrad)" opacity="0.2"/>
  <circle cx="150" cy="100" r="15" fill="url(#aiGrad)"/>
  
  <!-- Context nodes around the brain -->
  <g id="context-nodes">
    <!-- Document node -->
    <rect x="80" y="60" width="20" height="25" rx="3" fill="#10b981"/>
    <rect x="83" y="65" width="14" height="1" fill="white"/>
    <rect x="83" y="68" width="10" height="1" fill="white"/>
    <rect x="83" y="71" width="12" height="1" fill="white"/>
    
    <!-- Code node -->
    <rect x="200" y="60" width="20" height="25" rx="3" fill="#f59e0b"/>
    <text x="210" y="75" text-anchor="middle" font-family="monospace" font-size="8" fill="white">&lt;/&gt;</text>
    
    <!-- Data node -->
    <circle cx="90" cy="140" r="12" fill="#ef4444"/>
    <rect x="85" y="136" width="10" height="8" rx="1" fill="white"/>
    
    <!-- API node -->
    <circle cx="210" cy="140" r="12" fill="#7c3aed"/>
    <text x="210" y="145" text-anchor="middle" font-family="Arial, sans-serif" font-size="8" fill="white">API</text>
  </g>
  
  <!-- Connection lines from nodes to brain -->
  <g id="connections" stroke="#2563eb" stroke-width="2" opacity="0.4">
    <line x1="100" y1="75" x2="135" y2="90"/>
    <line x1="200" y1="75" x2="165" y2="90"/>
    <line x1="102" y1="130" x2="135" y2="110"/>
    <line x1="198" y1="130" x2="165" y2="110"/>
  </g>
  
  <!-- Processing waves from brain -->
  <g id="processing" stroke="#2563eb" stroke-width="1" opacity="0.6" fill="none">
    <circle cx="150" cy="100" r="20" stroke-dasharray="3,3"/>
    <circle cx="150" cy="100" r="30" stroke-dasharray="5,5"/>
    <circle cx="150" cy="100" r="40" stroke-dasharray="7,7"/>
  </g>
  
  <!-- Title -->
  <text x="150" y="165" text-anchor="middle" font-family="Arial, sans-serif" font-size="14" font-weight="bold" fill="#1f2937">
    AI Context Engine
  </text>
</svg>